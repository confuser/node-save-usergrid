  "description": "Usergrid engine for save crud operations",
  "main": "index.js",
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
