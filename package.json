    "memoizee": "^0.4.1",
    "request": "2.81.0"
  },
  "devDependencies": {
    "@confuser/eslint-config": "^2.0.0",
    "coveralls": "^2.11.2",
    "eslint": "^2.10.2",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.2",
    "nock": "^9.0.2"
